{
  "name": "@xmcl/unzip",
  "version": "2.0.0",
  "main": "./index.ts",
  "description": "A (hopefully) easy unzip interface based on yauzl",
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "build:type": "tsc",
    "build:cjs": "esbuild --target=node16 --platform=node --external:yauzl --sourcemap --format=cjs --bundle --outfile=dist/index.js index.ts",
    "build:esm": "esbuild --target=node16 --platform=node --external:yauzl --sourcemap --format=esm --bundle --outfile=dist/index.mjs index.ts"
  },
  "publishConfig": {
    "main": "./dist/index.js",
    "module": "./dist/index.mjs",
    "access": "public"
  },
  "peerDependencies": {
    "yauzl": "^2.10.0"
  },
  "devDependencies": {
    "@types/node": "^15.14.9",
    "@types/uuid": "^9.0.1",
    "@types/yauzl": "^2.10.0",
    "yauzl": "^2.10.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Voxelum/minecraft-launcher-core-node.git"
  },
  "author": "cijhn@hotmail.com",
  "sideEffects": false,
  "keywords": [
    "typescript",
    "nodejs",
    "yauzl",
    "unzip",
    "zlib",
    "extract"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Voxelum/minecraft-launcher-core-node/issues"
  },
  "homepage": "https://github.com/Voxelum/minecraft-launcher-core-node#readme",
  "dependencies": {
    "@types/node": "~18.15.11",
    "@xmcl/eslint-config": "workspace:^0.0.1",
    "esbuild": "^0.17.14",
    "eslint": "^8.37.0",
    "typescript": "^5.0.3"
  }
}
